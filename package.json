{
  "name": "caldate",
  "version": "1.0.3",
  "description": "calendar date for date-holidays",
  "keywords": [
    "calendar",
    "date"
  ],
  "homepage": "https://github.com/commenthol/caldate",
  "bugs": {
    "url": "https://github.com/commenthol/caldate/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/commenthol/caldate.git"
  },
  "license": "ISC",
  "author": "commenthol <commenthol@gmail.com>",
  "main": "lib",
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "scripts": {
    "all": "npm-run-all clean lint build test",
    "build": "babel -d lib src",
    "clean": "rimraf lib es coverage .nyc_output",
    "clean:all": "npm-run-all clean clean:node_modules",
    "clean:node_modules": "rimraf node_modules",
    "coverage": "nyc -r text -r html npm test",
    "lint": "eslint '**/*.js'",
    "prepublishOnly": "npm run all",
    "readme": "markedpp --githubid -i README.md -o README.md",
    "test": "mocha"
  },
  "babel": {
    "presets": [
      "env"
    ]
  },
  "eslintConfig": {
    "extends": "standard",
    "plugins": [
      "standard"
    ]
  },
  "dependencies": {
    "moment-timezone": "^0.5.23"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "eslint": "^5.9.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "mocha": "^5.2.0",
    "npm-run-all": "^4.1.3",
    "nyc": "^13.1.0",
    "rimraf": "^2.6.2"
  },
  "engines": {
    "node": ">=4.0.0"
  },
  "maintainers": [
    "commenthol <commenthol@gmail.com>"
  ]
}
